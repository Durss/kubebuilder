package com.muxxu.kube.common.components.form {	import com.muxxu.kube.kubebuilder.graphics.RadioButtonSelectedSkin;	import com.muxxu.kube.kubebuilder.graphics.RadioButtonSkin;	import com.nurun.components.button.visitors.CssVisitor;	import com.nurun.components.button.visitors.FrameVisitor;	import com.nurun.components.button.visitors.FrameVisitorOptions;	import com.nurun.components.form.FormComponentGroup;	import com.nurun.components.form.RadioButton;	import flash.display.MovieClip;	/**	 * Creates a preskined RadioButton instance.	 * 	 * @author  Francois	 */	public class KubeRadioButton extends RadioButton {										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>KubeRadioButton</code>.		 */		public function KubeRadioButton(label:String, group:FormComponentGroup) {			group.add(this);			super(label, "radioButton", "radioButton_selected", new RadioButtonSkin(), new RadioButtonSelectedSkin());			var fv:FrameVisitor = new FrameVisitor();			var opts:FrameVisitorOptions = new FrameVisitorOptions("out", "over", "down", "disable");			fv.addTarget(defaultIcon as MovieClip, opts);			fv.addTarget(selectedIcon as MovieClip, opts);			yLabelOffset = -3;			accept(fv);			accept(new CssVisitor());		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */			}}